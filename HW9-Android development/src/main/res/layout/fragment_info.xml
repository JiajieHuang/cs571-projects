<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Info"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/section_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="460dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.272">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:text="Address" />

            <TextView
                android:id="@+id/address"
                android:layout_width="243dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:text="No address" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView9"
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:text="Phone Number" />

            <TextView
                android:id="@+id/phone_number"
                android:layout_width="245dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:text="No phone number" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView11"
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:text="Price Level" />

            <TextView
                android:id="@+id/price_level"
                android:layout_width="243dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:text="No price level" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView13"
                android:layout_width="125dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:text="Rating" />

            <RatingBar
                android:id="@+id/MyRating"
                android:layout_width="wrap_content"
                android:layout_height="69dp"
                android:layout_weight="0"
                android:numStars="5"
                android:scaleX="0.6"
                android:scaleY="0.6"
                android:stepSize="0.1"
                android:transformPivotX="0dp"
                android:transformPivotY="0dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView16"
                android:layout_width="124dp"
                android:layout_height="35dp"
                android:layout_weight="0"
                android:text="Google Page" />

            <TextView
                android:id="@+id/google_page"
                android:layout_width="241dp"
                android:layout_height="35dp"
                android:layout_weight="0"
                android:text="No google page" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="37dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView18"
                android:layout_width="124dp"
                android:layout_height="36dp"
                android:layout_weight="0"
                android:text="website" />

            <TextView
                android:id="@+id/website"
                android:layout_width="243dp"
                android:layout_height="36dp"
                android:layout_weight="0"
                android:text="No website" />
        </LinearLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>